<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World!"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.berlski.custom.widget.InputInfoSwitchView
        android:layout_width="match_parent"
        android:layout_height="46dp"
        app:iisv_is_required="true"
        app:iisv_item_one="@string/app_name"
        app:iisv_item_two="@string/app_name"
        app:iisv_name="@string/app_name" />

    <com.berlski.custom.widget.InfoEntryView
        android:layout_width="match_parent"
        android:layout_height="46dp"
        app:iev_is_required="true"
        app:iev_name="@string/item_name" />

</LinearLayout>