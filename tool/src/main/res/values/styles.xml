<resources>

    <!--项目选择的条目view-->
    <declare-styleable name="SwitchOptionItemView">
        <attr name="soiv_name" format="string" /><!--字典名称-->
        <attr name="soiv_is_required" format="boolean" /><!--是否必选-->
        <attr name="soiv_default_select" format="integer" /><!--默认选中项-->

        <attr name="soiv_option_one" format="string" /><!--选项一-->
        <attr name="soiv_option_two" format="string" /><!--选项二-->
        <attr name="soiv_option_three" format="string" /><!--选项三-->
        <attr name="soiv_option_four" format="string" /><!--选项四-->
        <attr name="soiv_option_five" format="string" /><!--选项五-->
    </declare-styleable>

    <!--选择项目的view-->
    <declare-styleable name="SwitchView">
        <attr name="sv_default_select" format="integer" /><!--默认选中项-->

        <attr name="sv_option_one" format="string" /><!--选项一-->
        <attr name="sv_option_two" format="string" /><!--选项二-->
        <attr name="sv_option_three" format="string" /><!--选项三-->
        <attr name="sv_option_four" format="string" /><!--选项四-->
        <attr name="sv_option_five" format="string" /><!--选项五-->
    </declare-styleable>

    <!-- 信息输入的条目view-->
    <declare-styleable name="InfoEntryItemView">
        <attr name="ieiv_name" format="string" /><!--条目名称-->
        <attr name="ieiv_is_required" format="boolean" /><!--是否必选-->
        <attr name="ieiv_hint" format="string" /><!--提示文字-->
        <attr name="ieiv_length" format="integer" /><!--文字文字长度-->
        <attr name="ieiv_postfix" format="string" /><!--后缀，如：单位-->

        <attr name="ieiv_type">
            <flag name="TEXT" value="0x0001" /><!--文本文字-->
            <flag name="NAME" value="0x0002" /><!--姓名-->
            <flag name="EMAIL" value="0x0003" /><!--电子邮箱-->
            <flag name="PHONE_NO" value="0x0004" /><!--手机号-->
            <flag name="CREDENTIAL_NO" value="0x0005" /><!--证件号-->
        </attr>

        <attr name="ieiv_credential_type">
            <flag name="ID_CARD" value="0x0021" /><!--身份证-->
            <flag name="PASSPORT" value="0x0022" /><!--护照-->
            <flag name="HONGKONG_AND_MACAO_PASS" value="0x0023" /><!--港澳通行证-->
            <flag name="TAIWAN_COMPATRIOT_CARD" value="0x0024" /><!--台湾同胞证-->
        </attr>
    </declare-styleable>

    <!--阴影按钮-->
    <declare-styleable name="ShadowButton">
        <attr name="sb_text" format="string" />
        <attr name="sb_textColor" format="color" />
        <attr name="sb_textSize" format="dimension" />
    </declare-styleable>

    <!--选择list下bean对象的条目view-->
    <declare-styleable name="SelectBeanItemView">
        <attr name="sbiv_name" format="string" /><!--字典名称-->
        <attr name="sbiv_name_color" format="color" /><!--条目名字体颜色-->
        <attr name="sbiv_name_size" format="dimension" /><!--条目名字体大小-->

        <attr name="sbiv_hint" format="string" /><!--提示文字-->
        <attr name="sbiv_hint_color" format="color" /><!--提示文字颜色-->
        <attr name="sbiv_hint_size" format="dimension" /><!--提示文字大小-->

        <attr name="sbiv_is_multiple_select" format="boolean" /><!--是否多选-->

        <attr name="sbiv_right_icon" format="reference" /><!--资源id-->
        <attr name="sbiv_right_icon_size" format="dimension" /><!--资源id大小-->

        <attr name="sbiv_is_required" format="boolean" /><!--是否必选-->
        <attr name="sbiv_required_icon_size" format="dimension" /><!--资源id大小-->
        <attr name="sbiv_required_margin_start" format="dimension" /><!--必选标记居左-->

        <attr name="sbiv_content_color" format="color" /><!--内容字体颜色-->
        <attr name="sbiv_content_size" format="dimension" /><!--内容文字大小-->
        <attr name="sbiv_content_margin_end" format="dimension" /><!--内容居右-->
    </declare-styleable>


    <!--选择list下bean对象的条目view-->
    <declare-styleable name="ClickItemView">
        <attr name="civ_name" format="string" /><!--字典名称-->
        <attr name="civ_name_color" format="color" /><!--条目名字体颜色-->
        <attr name="civ_name_size" format="dimension" /><!--条目名字体大小-->

        <attr name="civ_hint" format="string" /><!--提示文字-->
        <attr name="civ_hint_color" format="color" /><!--提示文字颜色-->
        <attr name="civ_hint_size" format="dimension" /><!--提示文字大小-->

        <attr name="civ_right_icon" format="reference" /><!--资源id-->
        <attr name="civ_right_icon_size" format="dimension" /><!--资源id大小-->

        <attr name="civ_is_required" format="boolean" /><!--是否必选-->
        <attr name="civ_is_required_preposition" format="boolean" /><!--是否必选前置-->
        <attr name="civ_required_icon_size" format="dimension" /><!--资源id大小-->
        <attr name="civ_required_margin" format="dimension" /><!--必选标记居左-->

        <attr name="civ_content_color" format="color" /><!--内容字体颜色-->
        <attr name="civ_content_size" format="dimension" /><!--内容文字大小-->
        <attr name="civ_content_margin_end" format="dimension" /><!--内容居右-->
    </declare-styleable>

    <!--筛选蓝，下拉菜单开关-->
    <declare-styleable name="MenuButton">
        <attr name="mb_text" format="string" />
        <attr name="mb_text_size" format="dimension" />
        <attr name="mb_text_open_color" format="color" />
        <attr name="mb_text_close_color" format="color" />
        <attr name="mb_icon_size" format="dimension" />
        <attr name="mb_icon_open_color" format="color" />
        <attr name="mb_icon_close_color" format="color" />
        <attr name="mb_icon_open_icon" format="reference" />
        <attr name="mb_icon_close_icon" format="reference" />
        <attr name="mb_is_show_bottom_line" format="boolean" />
        <attr name="mb_bottom_line_color" format="color" />
        <attr name="mb_bottom_line_height" format="dimension" />
    </declare-styleable>

    <!--语音输入的条目view-->
    <declare-styleable name="SpeechInputItemView">
        <attr name="siiv_name" format="string" /><!--名称-->
        <attr name="siiv_is_required" format="boolean" /><!--是否必选-->
        <attr name="siiv_is_voice" format="boolean" /><!--是否语音-->
        <attr name="siiv_hint" format="string" /><!--提示文字-->
        <attr name="siiv_edit_background" format="reference" /><!--输入框背景-->
        <attr name="siiv_edit_padding" format="reference" /><!--输入框内边距-->
        <attr name="siiv_min_height" format="dimension" /><!--最低高度-->
    </declare-styleable>


    <!--数字区间滑块的条目view-->
    <declare-styleable name="NumberRangeItemView">
        <attr name="nriv_name" format="string" /><!--条目名称-->
        <attr name="nriv_is_required" format="boolean" /><!--是否必选-->

        <attr name="nriv_min_hint" format="string" /><!--最小数提示文字-->
        <attr name="nriv_min_text" format="string" /><!--最小数文字-->
        <attr name="nriv_max_hint" format="string" /><!--最大数提示文字-->
        <attr name="nriv_max_text" format="string" /><!--最大数文字-->

        <attr name="nriv_min_limit" format="integer" /><!--最小数字限定-->
        <attr name="nriv_max_limit" format="integer" /><!--最大数字限定-->
    </declare-styleable>


    <!--数字区间滑块view-->
    <declare-styleable name="NumberRangeSliderView">
        <attr name="nrsv_min" format="integer" />
        <attr name="nrsv_max" format="integer" />
        <attr name="nrsv_unpressedTargetRadius" format="dimension" />
        <attr name="nrsv_pressedTargetRadius" format="dimension" />
        <attr name="nrsv_targetColor" format="color" />
        <attr name="nrsv_insideRangeLineColor" format="color" />
        <attr name="nrsv_outsideRangeLineColor" format="color" />
        <attr name="nrsv_insideRangeLineStrokeWidth" format="dimension" />
        <attr name="nrsv_outsideRangeLineStrokeWidth" format="dimension" />
    </declare-styleable>
</resources>
